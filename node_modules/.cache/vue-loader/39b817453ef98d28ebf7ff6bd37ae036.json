ort { eventBus } from \"../main\";\n\nexport default {\n  name: \"SearchByTags\",\n  data() {\n    return {\n      tag: \"\",\n      tags: [],\n      selectedTags: [],\n    };\n  },\n  created: function () {\n    this.getTags();\n    eventBus.$on(\"refresh\", () => {\n      this.selectedTags = [];\n    });\n  },\n  methods: {\n    getTags() {\n      // gets the tags for dropdown options\n      axios\n        .get(\"api/tags\")\n        .then((response) => {\n          this.tags = response.data;\n        })\n        .catch((error) => {\n          eventBus.$emit(\"error-msg\", {\n            errorMsg: error.response.data.error,\n          });\n        });\n    },\n    search() {\n      eventBus.$emit(\"search-by-tags\", { tags: this.selectedTags });\n    },\n  },\n  mounted: function () {\n    this.getTags();\n  },\n};\n</script>\n\n<style scoped>\n.v-card {\n  width: 40%;\n  min-width: 250px;\n  margin-top: 28px;\n  margin-bottom: 16px;\n}\n::v-deep .tag-checkbox .v-label {\n  font-size: 14px;\n}\n</style>\n"]}]}